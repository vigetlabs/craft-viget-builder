{
    "name": "viget/craft-viget-generator",
    "description": "Viget Base Module",
    "type": "yii2-extension",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms"
    ],
    "support": {
        "docs": "https://TODO",
        "issues": "https://TODO"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Viget",
            "homepage": "https://www.viget.com/"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0-alpha"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^4.1.0",
        "codeception/codeception": "^4.1.1",
        "codeception/module-asserts": "^1.3.1",
        "codeception/module-yii2": "^1.1.5",
        "codeception/module-phpbrowser": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "viget\\generator\\": "src/",
            "vigetgenerator\\fixtures\\": "tests/fixtures/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "extra": {
        "bootstrap": "viget\\generator\\Module"
    }
}
